{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "functions": { "api/index.py": { "runtime": "@vercel/python" } },
    "env": {
      "FLASK_APP": "app.py",
      "FLASK_ENV": "production"
    }
  }
  